@article{bib:Beck16,
author = {Beck, Kristin M. and Hosseini, Mahdi and Duan, Yiheng and Vuleti?, Vladan}, 
title = {Large conditional single-photon cross-phase modulation},
volume = {113}, 
number = {35}, 
pages = {9740-9744}, 
year = {2016}, 
doi = {10.1073/pnas.1524117113}, 
abstract ={Deterministic optical quantum logic requires a nonlinear quantum process that alters the phase of a quantum optical state by π through interaction with only one photon. Here, we demonstrate a large conditional cross-phase modulation between a signal field, stored inside an atomic quantum memory, and a control photon that traverses a high-finesse optical cavity containing the atomic memory. This approach avoids fundamental limitations associated with multimode effects for traveling optical photons. We measure a conditional cross-phase shift of π/6 (and up to π/3 by postselection on photons that remain in the system longer than average) between the retrieved signal and control photons, and confirm deterministic entanglement between the signal and control modes by extracting a positive concurrence. By upgrading to a state-of-the-art cavity, our system can reach a coherent phase shift of π at low loss, enabling deterministic and universal photonic quantum logic.}, 
URL = {http://www.pnas.org/content/113/35/9740.abstract}, 
eprint = {http://www.pnas.org/content/113/35/9740.full.pdf}, 
journal = {Proceedings of the National Academy of Sciences} 
}

