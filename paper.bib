@article {bib:Schwartz16,
	author = {Schwartz, I. and Cogan, D. and Schmidgall, E. R. and Don, Y. and Gantz, L. and Kenneth, O. and Lindner, N. H. and Gershoni, D.},
	title = {Deterministic generation of a cluster state of entangled photons},
	year = {2016},
	doi = {10.1126/science.aah4758},
	publisher = {American Association for the Advancement of Science},
	abstract = {Photonic cluster states are a resource for quantum computation based solely on single photon measurements. We use semiconductor quantum dots to deterministically generate long strings of polarization entangled photons in a cluster state by periodic timed excitation of a precessing matter qubit. In each period, an entangled photon is added to the cluster state formed by the matter qubit and the previously emitted photons. In our prototype device, the qubit is the confined dark exciton, and it produces strings of hundreds of photons in which the entanglement persists over five sequential photons. The measured process map characterizing the device has a fidelity of 0.81 with that of an ideal device. Further feasible improvements of this device may reduce the resources needed for optical quantum information processing.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/early/2016/09/07/science.aah4758},
	eprint = {http://science.sciencemag.org/content/early/2016/09/07/science.aah4758.full.pdf},
	journal = {Science}
}


