	Date-Modified = {2017-05-13 16:19:15 +0000},
	Isbn = {1749-4885},
	Journal = {Nat Photon},
	L3 = {10.1038/nphoton.2013.287; http://www.nature.com/nphoton/journal/v7/n12/abs/nphoton.2013.287.html#supplementary-information},
	M3 = {Letter},
	Month = {12},
	Number = {12},
	Pages = {982--986},
	Publisher = {Nature Publishing Group},
	Title = {Ultra-large-scale continuous-variable cluster states multiplexed in the time domain},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1038/nphoton.2013.287},
	Volume = {7},
