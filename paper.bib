@article{bib:Chen17,
author = {Luo-Kan Chen and Zheng-Da Li and Xing-Can Yao and Miao Huang and Wei Li and He Lu and Xiao Yuan and Yan-Bao Zhang and Xiao Jiang and Cheng-Zhi Peng and Li Li and Nai-Le Liu and Xiongfeng Ma and Chao-Yang Lu and Yu-Ao Chen and Jian-Wei Pan},
journal = {Optica},
keywords = {Quantum optics; Quantum information and processing},
number = {1},
pages = {77--83},
publisher = {OSA},
title = {Observation of ten-photon entanglement using thin BiB3O6 crystals},
volume = {4},
month = {Jan},
year = {2017},
url = {http://www.osapublishing.org/optica/abstract.cfm?URI=optica-4-1-77},
doi = {10.1364/OPTICA.4.000077},
abstract = {Coherently manipulating a number of entangled qubits is the key task of quantum information processing. In this paper, we report on the experimental realization of a ten-photon Greenberger--Horne--Zeilinger state using thin BiB3O6 crystals. The observed fidelity is 0.606{\textpm}0.029, demonstrating a genuine entanglement with a standard deviation of 3.6$\sigma$. This result is further verified using p-value calculation, obtaining an upper bound of 3.7{\texttimes}10{\textminus}3 under an assumed hypothesis test. Our experiment paves a new way to efficiently engineer BiB3O6 crystal-based multi-photon entanglement systems, which provides a promising platform for investigating advanced optical quantum information processing tasks such as boson sampling, quantum error correction, and quantum-enhanced measurement.},
}


